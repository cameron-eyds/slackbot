{
  "name": "freshworks-slack",
  "version": "1.0.0",
  "description": "Boilerplate for Slack projects",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node -r dotenv/config src/index.js",
    "build": "babel src --out-dir dist",
    "serve": "cross-env NODE_ENV=production node -r dotenv/config dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@slack/bolt": "1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "nodemon": "2.0.2",
    "serverless-dotenv-plugin": "2.1.1",
    "serverless-offline": "12.0.4"
  }
}
